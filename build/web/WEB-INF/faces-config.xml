<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
<navigation-rule>
        <navigation-case>
            <from-outcome>yes</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        </navigation-rule>
       
        <navigation-rule>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/login.xhtml</to-view-id>
        </navigation-case>
        </navigation-rule>
       <navigation-rule>
           
        <navigation-case>
            <from-outcome>yesadmin</from-outcome>
            <to-view-id>/news_posting.xhtml</to-view-id>
        </navigation-case>
        </navigation-rule>
       
    
    <managed-bean>
        <managed-bean-name>b1</managed-bean-name>
        <managed-bean-class>bean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
     <managed-bean>
        <managed-bean-name>b2</managed-bean-name>
        <managed-bean-class>bean2</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>b3</managed-bean-name>
        <managed-bean-class>bean3</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
